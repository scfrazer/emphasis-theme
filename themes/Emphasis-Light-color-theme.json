{
	"name": "EmphasisLight",
	"type": "light",
	"include": "./light_plus.json",
	"colors": {
		"sideBar.background": "#ffffff",
		"sideBar.foreground": "#000000",
		"sideBar.border": "#c0c0c0",
		"sideBarSectionHeader.background": "#d4d4d4",
		"list.activeSelectionIconForeground": "#fff",
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"editorCursor.foreground": "#ff0000",
		"editorGutter.background": "#f8f8f8",
		"editorLineNumber.foreground": "#c0c0c0",
		"editorLineNumber.activeForeground": "#000000",
		"editorBracketMatch.border": "#00ff00",
		"gitDecoration.addedResourceForeground": "#00d000",
		"gitDecoration.modifiedResourceForeground": "#0000d0",
		"gitDecoration.deletedResourceForeground": "#d00000",
		"gitDecoration.renamedResourceForeground": "#8b008b",
		"gitDecoration.untrackedResourceForeground": "#c0c000",
		"gitDecoration.ignoredResourceForeground": "#909090",
		"gitDecoration.conflictingResourceForeground": "#d00000",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiRed": "#cd0000",
		"terminal.ansiGreen": "#005f00",
		"terminal.ansiYellow": "#8b8b00",
		"terminal.ansiBlue": "#00008b",
		"terminal.ansiMagenta": "#8b008b",
		"terminal.ansiCyan": "#008b8b",
		"terminal.ansiWhite": "#ffffff",
		"terminal.ansiBrightBlack": "#555555",
		"terminal.ansiBrightRed": "#ee0000",
		"terminal.ansiBrightGreen": "#00ee00",
		"terminal.ansiBrightYellow": "#eeee00",
		"terminal.ansiBrightBlue": "#0000ee",
		"terminal.ansiBrightMagenta": "#ee00ee",
		"terminal.ansiBrightCyan": "#00eeee",
		"terminal.ansiBrightWhite": "#e5e5e5",
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a8a8a8"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": [
				"comment.block.preprocessor",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#8b8b00"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a8a8a8"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage",
				"keyword.operator.logical.python",
				"storage.type.systemverilog",
				"storage.type.class.systemverilog",
				"storage.type.function.systemverilog",
				"storage.type.task.systemverilog",
				"storage.type.coverage.systemverilog"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#00008b"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"storage.modifier",
				"support.type"
			],
			"settings": {
				"foreground": "#00688b"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language",
				"variable.parameter.function.language.special.self.python",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#551a8b"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable",
				"storage.type.variable.event.systemverilog"
			],
			"settings": {
				"foreground": "#000000",
				"fontStyle": ""
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter",
			],
			"settings": {
				"foreground": "#006b00"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#8b0000"
			}
		},
		{
			"name": "Labels",
			"scope": [
				"entity.name.function.endlabel.systemverilog"
			],
			"settings": {
				"foreground": "#b03060",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#00688b"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#d00000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#00688b"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#551a8b"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#91B3E0"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#91B3E0"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		}
	]
}
